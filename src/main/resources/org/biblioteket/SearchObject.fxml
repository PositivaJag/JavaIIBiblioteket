<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.biblioteket.SearchObjectController">
   <children>
      <TextField fx:id="searchField" layoutX="178.0" layoutY="38.0" onAction="#writeSearchField" prefHeight="25.0" prefWidth="244.0" promptText="sök" />
      <Button fx:id="buttonSearch" layoutX="431.0" layoutY="38.0" mnemonicParsing="false" onAction="#clickButtonSearch" prefHeight="25.0" prefWidth="68.0" text="Sök" />
      <Button fx:id="buttonUpdate" layoutX="480.0" layoutY="361.0" mnemonicParsing="false" onAction="#clickButtonUpdate" text="Uppdatera" />
      <Button fx:id="buttonExit" layoutX="553.0" layoutY="6.0" mnemonicParsing="false" onAction="#clickButtonExit" prefHeight="25.0" prefWidth="21.0" text="X" textAlignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Label focusTraversable="false" layoutX="43.0" layoutY="40.0" text="Skriv in sökord här:  ">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="SE_RESIZE" />
         </cursor>
      </Label>
      <TabPane fx:id="tabPane" layoutX="24.0" layoutY="67.0" prefHeight="290.0" prefWidth="529.0">
         <tabs>
            <Tab fx:id="tabAlla" closable="false" text="Alla träffar">
               <content>
                  <TableView fx:id="tableAlla" prefHeight="278.0" prefWidth="529.0" />
               </content>
            </Tab>
            <Tab fx:id="tabBok" closable="false" text="Böcker">
               <content>
                  <TableView fx:id="tableBok" prefHeight="278.0" prefWidth="529.0" />
               </content>
            </Tab>
            <Tab fx:id="tabFilm" closable="false" text="Filmer">
               <content>
                  <TableView fx:id="tableFilm" prefHeight="227.0" prefWidth="529.0" />
               </content>
            </Tab>
            <Tab fx:id="tabTidning" closable="false" text="Tidskrifter">
               <content>
                  <TableView fx:id="tableTidning" prefHeight="278.0" prefWidth="529.0" />
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</AnchorPane>
